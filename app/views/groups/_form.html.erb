<div class="container rounded bg-light" style="background-color:#e9ecef; padding:8px; height: 50%">
<%= form_for(@group) do |f| %>
  <div class="form" style="height: 50%;">
  <div class="form-group">
   <h5> <%= f.label :name %></h5>
    <%= f.text_field :name %>
  </div>

  <div class="form-group">
  <h5><%= f.label 'Add a New Member'%></h5>
  
    <%= collection_select(:user, :id, @group.not_in_group, :id, :name) %>
  
  </div>
  <%# <%= f.select(:users, @users, {}, {multiple: true, selected: @group.not_in_group }) %> 
  <%# options for select and provide the array of selected users????%>
  <div class="form-group">
    <div class="actions" style="font-size:18px; margin-top: 40px;">
      <%= f.submit %>
    </div>
    
  </div>
  </div>
<% end %>
</div>