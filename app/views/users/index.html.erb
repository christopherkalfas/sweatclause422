  
<p id="notice"><%= notice %></p>

<div class="card shadow-lg rounded"
    style="border: 1px;
        width: 50%;
    ">
<h3 class="card-header bg-dark text-light">Users</h3>
  <ul class="list-group list-group-flush">
    <% @users.each do |user| %>      
        <li class="list-group-item">
          <h4>
            <%= link_to(user.username, user_path(user)) %>
          </h4>
        </li>
    <% end %>
  </ul>




<h5 class="card-footer"><%= link_to('New User', new_user_path) %></h5>
</div>